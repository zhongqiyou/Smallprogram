page{
  background:#eee;
}
.item{
  line-height: 50px;
  border-bottom: 1px solid #ccc;
  color: #777;
  overflow: hidden;
  background: #fff;
}
.item-btn{
  margin:10px 10px 0px 10px;
  background: #ff9700;
  height: 45px;
  line-height: 45px;
  border-radius: 10px;
  color: #fff;
  text-align: center; 
}
.title{
  padding-left:10px;
  width: 90px;
  float: left;
}
.radio_group{
  float: left;
}
.input_text{
  height: 50px;
  float: left;
}
.res_text{
  text-align: center;
  line-height: 100px;

}
.res_icon{
  position:relative;
  top: 5px;
  margin-right:10px; 
}
.res_btn{
  margin: 0 10px;
}
.maptext{
  display: block;
  margin-top:10px;
  line-height: 30px; 
}